<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/datNhanh_FmLt_showChiTietSanPham"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:alpha="0.7"        android:background="@color/black" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical"><LinearLayout    android:id="@+id/datNhanh_linear_turnOffDiaLog"    android:layout_width="match_parent"    android:gravity="center"    android:paddingTop="30dp"    android:paddingBottom="10dp"    android:layout_height="50dp">    <LinearLayout        android:layout_width="40dp"        android:layout_height="3dp"        android:layout_gravity="center"        android:background="@drawable/broder_radius_white"        android:orientation="vertical"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.498"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" /></LinearLayout>        <androidx.constraintlayout.widget.ConstraintLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:background="@drawable/broder_radius_topleft_topright_white"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/linearLayout3">            <LinearLayout                android:id="@+id/linearLayout4"                android:layout_width="323dp"                android:layout_height="323dp"                android:layout_marginTop="40dp"                android:background="@color/lf_gray2"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <ImageView                    android:id="@+id/datNhanh_img_showAnhSanPham"                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_margin="10dp"                    android:src="@drawable/img_anhmau1" />            </LinearLayout>            <TextView                android:id="@+id/datNhanh_tv_xuatXuSanPham"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="20dp"                android:layout_marginTop="10dp"                android:text="Xuất xứ: Đà lạt"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintHorizontal_bias="0"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />            <TextView                android:id="@+id/datNhanh_tv_showTenSanPham"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="20dp"                android:text="Hồng đào khô"                android:textColor="@color/lf_orange"                android:textSize="27sp"                android:textStyle="bold|italic"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintHorizontal_bias="0 "                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/datNhanh_tv_xuatXuSanPham" />            <TextView                android:id="@+id/datNhanh_tv_giaSanPham"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="20dp"                android:text="100.000VND - "                android:textColor="#4E4D4D"                android:textSize="20sp"                android:textStyle="bold"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/datNhanh_tv_showTenSanPham" />            <TextView                android:id="@+id/datNhanh_tv_giamGiaSanPham"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="120.000VND"                android:textColor="#868383"                android:textSize="20sp"                android:textStyle="bold"                app:layout_constraintBottom_toBottomOf="@+id/datNhanh_tv_giaSanPham"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintHorizontal_bias="0.0"                app:layout_constraintStart_toEndOf="@+id/datNhanh_tv_giaSanPham"                app:layout_constraintTop_toTopOf="@+id/datNhanh_tv_giaSanPham" />            <TextView                android:id="@+id/datNhanh_tv_ngaySanXuat"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="20dp"                android:text="NSX:12/10/2021"                android:textColor="@color/lf_brown"                android:textSize="12sp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintHorizontal_bias="0"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/datNhanh_tv_giaSanPham" />            <TextView                android:id="@+id/datNhanh_tv_soLuongSanPhamTrongKho"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="1200Sp"                android:textColor="@color/lf_orange"                android:textSize="12sp"                app:layout_constraintBottom_toBottomOf="@+id/textView8"                app:layout_constraintStart_toEndOf="@+id/textView8"                app:layout_constraintTop_toTopOf="@+id/textView8"                app:layout_constraintVertical_bias="0.0" />            <TextView                android:id="@+id/textView8"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="136dp"                android:text="Số lượng trong kho: "                android:textColor="@color/lf_brown"                android:textSize="12sp"                app:layout_constraintBottom_toBottomOf="@+id/datNhanh_tv_ngaySanXuat"                app:layout_constraintStart_toEndOf="@+id/datNhanh_tv_ngaySanXuat"                app:layout_constraintTop_toTopOf="@+id/datNhanh_tv_ngaySanXuat" />            <ImageView                android:id="@+id/imageView"                android:layout_width="21dp"                android:layout_height="21dp"                android:layout_marginStart="20dp"                android:layout_marginLeft="20dp"                android:layout_marginTop="5dp"                android:src="@drawable/ic_giohang_tim"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/datNhanh_tv_ngaySanXuat" />            <TextView                android:id="@+id/datNhanh_tv_soLuongSanPhamYeuThichDaMua"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="12"                android:textColor="@color/lf_brown"                android:textSize="14sp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintHorizontal_bias="0.0"                app:layout_constraintStart_toEndOf="@+id/imageView"                app:layout_constraintTop_toTopOf="@+id/imageView" />            <LinearLayout                android:id="@+id/linearLayout12"                android:layout_width="match_parent"                android:layout_height="70dp"                android:layout_marginLeft="20dp"                android:layout_marginTop="5dp"                android:layout_marginRight="20dp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/imageView">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="Mô tả: "                    android:textColor="@color/lf_brown"                    android:textSize="12sp" />                <ScrollView                    android:layout_width="match_parent"                    android:layout_height="match_parent">                    <TextView                        android:id="@+id/datNhanh_tv_moTaSanPham"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="Công nghệ sấy hồng treo được chuyển giao trực tiếptừ Nhật Bản. Quản đốc người Nhật trực tiếp phụ trách quảnlý chế biến sản xuất. Hồng Đà Lạt tuyển lựa.Món ăn vặt ưathích, thích hợp tiêu dung hoặc làm quà tặng..."                        android:textSize="12sp" />                </ScrollView>            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="90dp"                android:layout_marginBottom="4dp"                android:orientation="horizontal"                android:paddingTop="17dp"                android:paddingBottom="17dp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintHorizontal_bias="0.0"                app:layout_constraintStart_toStartOf="parent">                <LinearLayout                    android:layout_width="0dp"                    android:layout_height="match_parent"                    android:layout_marginLeft="20dp"                    android:layout_marginRight="10dp"                    android:layout_weight="4"                    android:background="@drawable/broder_radius_xanhduong_nhe">                    <ImageView                        android:id="@+id/datNhanh_img_btn_giamSoLuongSanPham"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:padding="15dp"                        android:src="@drawable/ic_giam"                        android:layout_gravity="center_vertical"/>                    <TextView                        android:id="@+id/datNhanh_tv_SoLuongSanpham"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_gravity="center"                        android:layout_weight="1"                        android:gravity="center"                        android:text="1"                        android:textColor="@color/lf_brown"                        android:textSize="17sp"                        android:textStyle="bold|normal" />                    <ImageView                        android:id="@+id/datNhanh_img_btn_tangSoLuongSanPham"                        android:layout_width="0dp"                        android:layout_height="match_parent"                        android:layout_weight="1"                        android:padding="15dp"                        android:src="@drawable/ic_them" />                </LinearLayout>                <Button                    android:id="@+id/datNhanh_btn_themSanPhamVaoGioHang"                    android:layout_width="0dp"                    android:layout_height="match_parent"                    android:layout_marginLeft="10dp"                    android:layout_marginRight="20dp"                    android:layout_weight="6"                    android:text="Add to cart 100.000VND"                    android:textColor="@color/white"                    android:textSize="14sp"                    android:textStyle="bold"                    android:background="@drawable/broder_radius_cam_thuonghieu"                    android:gravity="center">                </Button>            </LinearLayout>        </androidx.constraintlayout.widget.ConstraintLayout>    </LinearLayout></FrameLayout>